{
  "name": "@glass-project/dsu-utils",
  "version": "0.2.3",
  "description": "Small library with utils for dsu projects",
  "main": "./src/cli.js",
  "module": "./dist/esm/index.min.esm.js",
  "publishConfig": {
    "@glass-project:registry": " https://gitlab.ubitech.eu/api/v4/projects/684/packages/npm/"
  },
  "scripts": {
    "test": "jest --coverage --passWithNoTests",
    "prepare-release": "npm run docs",
    "release": "./bin/tag-release.sh",
    "clean-publish": "npx clean-publish",
    "coverage": "npm test && jest-coverage-badges --input \"./workdocs/coverage/coverage-summary.json\" --output \"./workdocs/badges\"",
    "docs": "npm run coverage && npx rimraf ./docs/* && cp -r ./workdocs/assets ./docs/assets && npm install better-docs taffydb && npx markdown-include ./mdCompile.json && npx jsdoc -c jsdocs.json -t ./node_modules/better-docs && npm remove better-docs taffydb"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.ubitech.eu/glass-project/wallet/dsu-utils.git"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "keywords": [
    "OpenDSU",
    "Typescript",
    "TestRunner"
  ],
  "author": "PDM&FC",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.ubitech.eu/glass-project/wallet/dsu-utils/-/issues"
  },
  "homepage": "https://gitlab.ubitech.eu/glass-project/wallet/dsu-utils#readme",
  "devDependencies": {
    "jest": "^27.0.6",
    "jest-coverage-badges": "^1.1.2",
    "jest-junit": "^13.0.0",
    "jsdoc": "^3.6.7",
    "jsdoc-mermaid": "^1.0.0",
    "markdown-include": "^0.4.3",
    "np": "^7.5.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3"
  }
}
